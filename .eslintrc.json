{
    "env": {
        "node": true
    },
    "extends": "airbnb",
    "rules": {
        "indent": [
            "error"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "func-names": [
            "error",
            "never"
        ],
        "no-param-reassign": 0,
        "no-unused-vars": 0,
        "no-plusplus": 0,
        "no-shadow": 0
    }
}